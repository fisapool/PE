# Proxy Configuration - DO NOT ADD ACTUAL CREDENTIALS TO THIS FILE
# Copy to .env and add your credentials there

# Primary Proxy Provider
PRIMARY_PROXY_API_URL=https://proxy-provider.example.com/api
PRIMARY_PROXY_USERNAME=your_username
PRIMARY_PROXY_PASSWORD=your_password

# Fallback Proxy Provider
FALLBACK_PROXY_API_URL=https://fallback-provider.example.com/api
FALLBACK_PROXY_USERNAME=your_username
FALLBACK_PROXY_PASSWORD=your_password

# General Settings
PROXY_TYPE=residential
MAX_CONCURRENT_CONNECTIONS=10
ENABLE_BANDWIDTH_TRACKING=true 